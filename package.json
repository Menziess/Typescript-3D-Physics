{
  "name": "app",
  "version": "1.0.0",
  "description": "A react app",
  "main": "dist/app-client.js",
  "compilerOptions": {
    "skipLibCheck": true
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --watch dist dist/app-server.js",
    "watch": "tsc --watch",
    "watchify": "watchify dist/app-client.js -o public/js/bundle.js",
    "browserify": "browserify dist/app-client.js > public/js/bundle.js",
    "compile": "tsc",
    "dev": "concurrently --kill-others \"npm run watch\" \"npm run watchify\" \"npm run start\""
  },
  "keywords": [
    "app",
    "react"
  ],
  "author": "Stefan Schenk",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "ejs": "^2.3.33",
    "express": "^4.0.35",
    "express-rewrite": "0.0.4",
    "oimo": "^1.0.5",
    "react": "^15.0.2",
    "react-dom": "^15.4.2",
    "react-materialize": "^0.18.1",
    "react-router": "^2.0.46",
    "react-three-renderer": "^3.0.2",
    "socket.io": "^1.7.2",
    "three": "^0.84.0",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "@types/node": "^7.0.4",
    "@types/react": "^15.0.4",
    "@types/three": "^0.83.1",
    "browserify": "^14.0.0",
    "concurrently": "^3.1.0",
    "nodemon": "^1.2.1",
    "react-hot-loader": "^1.3.1",
    "typescript": "^2.1.5",
    "watchify": "^3.9.0"
  }
}
